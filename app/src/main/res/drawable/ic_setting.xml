<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="oval">
            <size
                android:height="30dp"
                android:width="30dp"/>
            <corners
                android:radius="15dp"/>
            <solid
                android:color="@color/white"/>
        </shape>
    </item>
    <item
        android:drawable="@drawable/ic_settings"
        android:top="3dp"
        android:right="3dp"
        android:left="3dp"
        android:bottom="3dp"/>
</layer-list>